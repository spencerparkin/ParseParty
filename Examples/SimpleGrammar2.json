{
  "initial_rule": "statement-list",
  "algorithm":  "quick",
  "rules": {
    "statement-list": [
      [ "statement", ";", "statement-list" ],
      [ "statement", ";" ]
    ],
    "statement": [
      [ "expression" ]
    ],
    "expression": [
      [ "(", "sub-expression", ")", "binary-operator", "expression" ],
      [ "(", "sub-expression", ")" ],
      [ "@identifier", "binary-operator", "expression" ],
      [ "@number", "binary-operator", "expression" ],
      [ "@identifier" ],
      [ "@number" ]
    ],
    "sub-expression": [
      [ "expression" ]
    ],
    "binary-operator": [
      [ "+" ],
      [ "-" ],
      [ "*" ],
      [ "/" ],
      [ "%" ],
      [ "=" ]
    ]
  }
}