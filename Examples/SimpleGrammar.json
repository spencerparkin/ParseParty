{
  "initial_rule": "statement-list",
  "algorithm": "quick",
  "rules": {
    "statement-list": [
      [ "statement", ";" ],
      [ "statement", ";", "statement-list" ]
    ],
    "statement": [
      [ "expression" ]
    ],
    "expression": [
      [ "@identifier" ],
      [ "@number" ],
      [ "assignment-expression" ],
      [ "math-expression" ],
      [ "(", "expression", ")" ]
    ],
    "assignment-expression": [
      [ "expression", "=", "expression" ]
    ],
    "math-expression": [
      [ "expression", "+", "expression" ],
      [ "expression", "-", "expression" ],
      [ "expression", "*", "expression" ],
      [ "expression", "/", "expression" ],
      [ "expression", "%", "expression" ]
    ]
  }
}